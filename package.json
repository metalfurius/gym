{
  "name": "my-workout-tracker",
  "version": "1.0.2",
  "description": "A modern, responsive workout tracking web application",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:all": "node scripts/run-tests.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "test:manual": "echo 'Open tests/manual/index.html in a browser to run manual tests'",
    "serve": "python3 -m http.server 8080",
    "lint": "eslint . --ext .js,.cjs",
    "lint:fix": "eslint . --ext .js,.cjs --fix",
    "format": "prettier --write \"js/**/*.js\" \"css/**/*.css\" \"*.html\" \"*.json\" --ignore-path .gitignore",
    "format:check": "prettier --check \"js/**/*.js\" \"css/**/*.css\" \"*.html\" \"*.json\" --ignore-path .gitignore",
    "build:web": "node scripts/build-web.cjs",
    "build:web:dev": "node scripts/build-web.cjs development",
    "build:web:staging": "node scripts/build-web.cjs staging",
    "build:web:prod": "node scripts/build-web.cjs production",
    "cap:sync": "npm run build:web && npx cap sync",
    "cap:sync:dev": "npm run build:web:dev && npx cap sync",
    "cap:sync:staging": "npm run build:web:staging && npx cap sync",
    "cap:build:ios": "npm run build:web && npx cap sync ios",
    "cap:build:ios:dev": "npm run build:web:dev && npx cap sync ios",
    "cap:build:android": "npm run build:web && npx cap sync android",
    "cap:build:android:dev": "npm run build:web:dev && npx cap sync android",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:icons": "node scripts/generate-icons.cjs"
  },
  "keywords": [
    "workout",
    "fitness",
    "tracker",
    "gym"
  ],
  "author": "metalfurius",
  "license": "SEE LICENSE IN README.md",
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "eslint": "^8.57.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/splash-screen": "^7.0.4"
  }
}
